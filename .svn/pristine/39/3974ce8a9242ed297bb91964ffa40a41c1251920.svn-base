					<td style="padding: 0.3rem;" onclick=OpenWindow("../mail/registForm.do?mFrom={{eno}}","메일보내기",1000,800)><div style="margin-top: 13px;margin-left: 10px">
						<i class="fas fa-envelope"></i></div></td>
