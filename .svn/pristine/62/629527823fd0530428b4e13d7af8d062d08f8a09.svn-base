function SignReq(){
	signDocForm.find("[name='title']").val($('input[name="title"]').val());
	signDocForm.find("[name='eno']").val($('input[name="eno"]').val());
	signDocForm.find("[name='signContent']").val($('textarea[name="signContent"]').val());

