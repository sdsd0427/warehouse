<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Hr-Mapper">

	<select id="selectHrList" resultType="hr">
		SELECT
		    hrcode
		   ,eno
		   ,state
		   ,hrtime
		   ,overtime
		   ,ontime
		   ,offtime
		FROM
		    hr
	    WHERE
	    	eno=#{eno}
	</select>

	<select id="selectHrType" resultType="hr">
		SELECT
		    hrcode
		   ,eno
		   ,state
		   ,hrtime
		   ,overtime
		   ,ontime
		   ,offtime
		FROM
		    hr
	    WHERE
	    	eno=#{eno}
	</select>

</mapper>















