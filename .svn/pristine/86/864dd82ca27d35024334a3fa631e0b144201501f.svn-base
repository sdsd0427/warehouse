<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Department-Mapper">
	<select id="selectDepartMentList" resultType="department">
		SELECT
		    dno
		   ,dname
		   ,manager
		   ,updno
		   ,createdate
		   ,enabled
		FROM
		    department
	</select>
</mapper>