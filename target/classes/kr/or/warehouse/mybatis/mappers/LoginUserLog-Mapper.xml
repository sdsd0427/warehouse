<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="LoginUserLog-Mapper">

	<update id="insertLoginUserLog">
		insert into loginlog(eno,phone,email,ipAddress,inDate)
		values(#{eno},#{phone},#{email},#{ipAddress},#{inDate})
	</update>

	<update id="deleteLoginUserLog">
		delete from loginlog
	</update>

</mapper>

